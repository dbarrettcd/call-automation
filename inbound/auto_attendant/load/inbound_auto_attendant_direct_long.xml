<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--

This SIPp scenario does a:
- INVITE
- 15 second pause
- press 0
- 15 second pause 
- press 5
- 15 second pause 
- press 5
- 15 second pause
- press 0
- 15 second pause
- press 0
- 15 second pause 
- press 5
- 15 second pause 
- press 5
- 15 second pause
- press 0
- 15 second pause
- press 0
- ends the call

-->

<scenario name="INVITE + LOOP AUTO ATTENDANT + HANGUP">
   
   <!-- ************************************************************* -->
   <!-- send INVITE                                                   -->
   <!-- ************************************************************* -->

   <send retrans="500">
      <![CDATA[

         INVITE sip:[field0]@[remote_ip] SIP/2.0
         Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
         From: "SIPp" <sip:SIPp@[local_ip]>;tag=[pid]SIPpTag09[call_number]
         To: <sip:[field0]@[remote_ip]:[local_port]>
         Call-ID: [call_id]
         CSeq: [cseq] INVITE
         Contact: "SIPp" <sip:SIPp@[local_ip]:[local_port];ob>
         Max-Forwards: 70
         Allow: PRACK, INVITE, ACK, BYE, CANCEL, UPDATE, INFO, SUBSCRIBE, NOTIFY, REFER, MESSAGE, OPTIONS
         Supported: replaces, 100rel, timer, norefersub
         Subject: Performance Test
         Min-SE: 90
         User-Agent: SIPp 3.9 
         Content-Type: application/sdp
         Content-Length: [len]
   
         v=0
         o=- 53655765 2353687637 IN IP[local_ip_type] [local_ip]
         s=testcall
         c=IN IP[media_ip_type] [media_ip]
         t=0 0
         b=AS:84n
         a=X-nat:0
         m=audio 4000 RTP/AVP 103 102 104 109 3 0 8 9 101
         b=TIAS:64000
         a=rtcp:4001 IN IP[local_ip_type] [local_ip]
         a=sendrecv
         a=rtpmap:103 speex/16000
         a=rtpmap:102 speex/8000
         a=rtpmap:104 speex/32000
         a=rtpmap:109 iLBC/8000
         a=fmtp:109 mode=30
         a=rtpmap:3 GSM/8000
         a=rtpmap:0 PCMU/8000
         a=rtpmap:8 PCMA/8000
         a=rtpmap:9 G722/8000
         a=rtpmap:101 telephone-event/8000
         a=fmtp:101 0-15

      ]]>
   </send>
   
   <!-- Some asterisk servers may send  100 - Trying, 180 - Ringing, and/or, 183-Session in Progress -->
   
   <recv response="100" optional="true">
   </recv>

   <recv response="180" optional="true">
   </recv>

   <recv response="183" optional="true">
   </recv>

   <recv response="200">
   </recv>

   
   <!-- ************************************************************* -->
   <!-- send ACK                                                      -->
   <!-- ************************************************************* -->

   <!-- ACK the RTP connection so asterisk does not boot us from the call -->
   <send>
      <![CDATA[

         ACK sip:[field0]@[remote_ip] SIP/2.0
         Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
         From: "SIPp"<sip:SIPp@[local_ip]>;tag=[call_number]
         To: <sip:[field0]@[remote_ip]>
         Call-ID: [call_id]
         CSeq: [cseq] ACK
         User-Agent: SIPp/Linux
         Content-Length: 0

     ]]>
   </send>

   <!-- Alternate pressing 0 and pressing 5 because Asterisk ignores the third consecutive press of the same number -->

   <pause milliseconds="15000" />  
    
   <!-- Press 0 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_0.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 5 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_5.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 5 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_5.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 0 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_0.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 0 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_0.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 5 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_5.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 5 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_5.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 0 -->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_0.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" />
   
   <!-- Press 0-->
   <nop>
      <action>
         <exec play_pcap_audio="pcap/dtmf_2833_0.pcap"/>
      </action>
   </nop>
   
   <pause milliseconds="15000" /> 


   <!-- ************************************************************* -->
   <!-- send BYE                                                      -->
   <!-- ************************************************************* -->

   <send>
      <![CDATA[

         BYE sip:[field0]@[remote_ip] SIP/2.0
         Via: SIP/2.0/[transport] [local_ip]:[local_port];branch=[branch]
         From: "SIPp"<sip:SIPp@[local_ip]>;tag=[call_number]
         To: <sip:[field0]@[remote_ip]>
         Call-ID: [call_id]
         CSeq: [cseq] BYE
         User-Agent: SIPp/Linux
         Content-Length: 0

     ]]>
   </send>

   <!-- some asterisk servers send 200 for their BYE response, others send 487  -->

   <recv response="487" optional="true" next="2">
   </recv>

   <recv response="200" >
   </recv>

   <label id="2"/>

</scenario>